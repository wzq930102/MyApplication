<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/nav_menu"
    android:divider="@drawable/divider"
    android:showDividers="middle"
    android:orientation="vertical">

    <TextView
        android:id="@+id/popup_memory"
        style="@style/xiaLaTextStyle"
        android:drawableLeft="@mipmap/nav_icon_memory"
        android:text="观看历史" />

    <TextView
        android:id="@+id/popup_collect"
        style="@style/xiaLaTextStyle"
        android:drawableLeft="@mipmap/nav_icon_collect"
        android:text="我的收藏" />


    <TextView
        android:id="@+id/popup_cache"
        style="@style/xiaLaTextStyle"
        android:drawableLeft="@mipmap/nav_icon_cache"
        android:text="我的缓存" />

    <TextView
        android:id="@+id/popup_local"
        style="@style/xiaLaTextStyle"
        android:drawableLeft="@mipmap/nav_icon_local"
        android:text="每日签到" />


</LinearLayout>